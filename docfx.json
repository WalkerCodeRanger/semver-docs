{
    "metadata": 
    [{
        "src": 
        [{
            "src": "src/v2.1.0",
            "files": [ "Semver/**.csproj" ]
        }],
        "dest": "v2.1.x",
        "force": true,
        "properties": { "TargetFramework": "net452" }
    }],
    "build":
    {
        "content":
        [
            {
                "files":
                [
                    "v*/**.yml",
                    "v*/*.md",
                    "toc.yml",
                    "*.md"
                ],
                "exclude": [ "README.md" ]
            }
        ],
        "resource":
        [{
            "files": [
                "images/**",
                "CNAME"
            ]
        }],
        "overwrite":
        [{
            "files": [ "apidoc/**.md" ],
            "exclude": [
                "obj/**",
                "docs/**"
            ]
        }],
        "dest": "docs",
        "globalMetadata":
        {
            "_disableContribution": true,
            "_appTitle": "semver"
        },
        "globalMetadataFiles": [ ],
        "fileMetadataFiles": [],
        "template": [ "default" ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}