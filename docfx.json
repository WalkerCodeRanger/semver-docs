{
    "metadata": 
    [{
        "src": 
        [{
            "files": [ "src/v2.0.x/Semver/**.csproj" ]
        }],
        "dest": "metadata/v2.0.x",
        "disableGitFeatures": false,
        "disableDefaultFilter": false,
        "force": true,
        "properties": { "TargetFramework": "net452" }
    }],
    "build":
    {
        "content":
        [
            {
                "src": "metadata/v2.0.x",
                "dest": "v2.0.x/api",
                "files":
                [
                    "**.yml",
                    "index.md"
                ]
            },
            {
                "files":
                [
                    "toc.yml",
                    "*.md"
                ],
                "exclude": [ "README.md" ]
            }
        ],
        "resource":
        [{
            "files": [
                "images/**",
                "CNAME"
            ]
        }],
        "overwrite":
        [{
            "files": [ "apidoc/**.md" ],
            "exclude": [
                "obj/**",
                "docs/**"
            ]
        }],
        "dest": "docs",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [ "statictoc" ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}