{
    "metadata": [{
        "src": [{
            "files": [
                "src/Semver/**.csproj"
            ]
        }],
        "dest": "api",
        "disableGitFeatures": false,
        "disableDefaultFilter": false,
        "force": true
    }],
    "build": {
        "content": [{
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [{
            "files": [
                "images/**",
                "CNAME"
            ]
        }],
        "overwrite": [{
            "files": [
                "apidoc/**.md"
            ],
            "exclude": [
                "obj/**",
                "docs/**"
            ]
        }],
        "dest": "docs",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}