{
    "build":
    {
        "content":
        [
            {
                "src": "metadata/v2.0.6/net452",
                "dest": "v2.0.x",
                "files": [ "**.yml" ],
                "version": "v2.0.x"
            },
            {
                "src": "metadata/release-2.1/net452",
                "dest": "dev",
                "files": [ "**.yml" ],
                "version": "dev"
            },
            {
                "files":
                [
                    "toc.yml",
                    "**/*.md"
                ],
                "exclude": [ "README.md" ]
            }
        ],
        "resource":
        [{
            "files": [
                "images/**",
                "CNAME"
            ]
        }],
        "overwrite":
        [{
            "files": [ "apidoc/**.md" ],
            "exclude": [
                "obj/**",
                "docs/**"
            ]
        }],
        "dest": "_site",
        "globalMetadata":
        {
            "_disableContribution": false,
            "_appTitle": "semver"
        },
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [ "default" ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}