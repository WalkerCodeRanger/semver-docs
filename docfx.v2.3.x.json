{
    "metadata": 
    [{
        "src": 
        [{
            "src": "src/v2.3.0/",
            "files": [ "Semver/**/net452/*.dll" ]
        }],
        "dest": "v2.3.x",
        "force": true,
        "properties": { "TargetFramework": "net452" },
        "shouldSkipMarkup": true
    }],
    "build":
    {
        "content":
        [{
            "files":
            [
                "v2.3.x/*.yml",
                "v2.3.x/*.md",
                "toc.yml",
                "*.md"
            ],
            "exclude": [ "README.md" ],
            "version": "v2.3.x"
        }],
        "resource":
        [{
            "files": [
                "images/**",
                "CNAME",
                "ranges/index.html"
            ]
        }],
        "overwrite":
        [{
            "files": [ "apidoc/**.md" ],
            "src": "v2.3.x",
            "dest": "v2.3.x",
            "version": "v2.3.x"
        }],
        "dest": "build/v2.3.x",
        "globalMetadata":
        {
            "_disableContribution": false,
            "_appTitle": "semver"
        },
        "globalMetadataFiles": [ ],
        "fileMetadataFiles": [],
        "template": [ "default" ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}